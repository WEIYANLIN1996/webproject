        window.wdgdb.atRequestSucceeded = Date.now();
        performance.timing.atRequestSucceeded = window.wdgdb.atRequestSucceeded;
        document.dispatchEvent(new CustomEvent('atRequestSucceeded', { bubbles: true}))
    } catch(e) {
    }
